/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
} 

page {
  width: 100%;
  height: 100%;
}

view {
  box-sizing: border-box;
}

.flex {
  display: flex;
  width: 100%;
  height: 100%;
  align-content: flex-start;
}

.flex-wrap {
  flex-wrap: wrap;
}

.flex-col {
  flex-direction: column;
}

.flex-main {
  flex: 1;
  width: 100%;
} 
